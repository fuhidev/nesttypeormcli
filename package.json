{
    "name": "vscode-nesttpyeorm",
    "displayName": "Nest typeorm",
    "description": "Quickly scaffold angular file templates",
    "version": "1.6.4",
    "icon": "images/icon.png",
    "publisher": "alexiv",
    "author": {
        "name": "Alexander Ivanichev"
    },
    "engines": {
        "vscode": "^1.22.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/ivalexa/vscode-angular2-files"
    },
    "bugs": {
        "url": "https://github.com/ivalexa/vscode-angular2-files/issues"
    },
    "keywords": [
    ],
    "categories": [
        "Other",
        "Programming Languages",
        "Snippets"
    ],
    "scripts": {
        "templates": "copyfiles templates/**/*.tmpl templates/**/*.tmpl ./out/src/",
        "vscode:prepublish": "npm run templates && tsc -p ./",
        "compile": "npm run templates && tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "activationEvents": [
        "*"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "configuration": {
            "type": "object",
            "title": "Angular Files menu option configuration",
            "properties": {
                "angular2-files.menu.controller": {
                    "type": "boolean",
                    "default": true,
                    "description": "Shows or hides the menu item."
                },
                "angular2-files.menu.service": {
                    "type": "boolean",
                    "default": true,
                    "description": "Shows or hides the menu item."
                },
                "angular2-files.menu.entity": {
                    "type": "boolean",
                    "default": true,
                    "description": "Shows or hides the menu item."
                },
                "angular2-files.menu.module": {
                    "type": "boolean",
                    "default": true,
                    "description": "Shows or hides the menu item."
                }
            }
        },
        "commands": [
            {
                "command": "extension.addAngular2Controller",
                "title": "Generate Controller"
            },
            {
                "command": "extension.addAngular2Service",
                "title": "Generate Service"
            },
            {
                "command": "extension.addAngular2Entity",
                "title": "Generate Entity"
            },
            {
                "command": "extension.addAngular2Module",
                "title": "Generate Module"
            }
        ],
        "submenus": [
            {
                "id": "nesttypeorm",
                "label": "Nest typeorm"
            }
        ],
        "menus": {
            "explorer/context": [                
                {
                    "submenu": "nesttypeorm",
                    "group": "1_nesttypeorm"
                }
            ],
            "nesttypeorm": [
                {
                    "when": "config.angular2-files.menu.controller",
                    "command": "extension.addAngular2Controller"
                },
                {
                    "when": "config.angular2-files.menu.service",
                    "command": "extension.addAngular2Service"
                },
                {
                    "when": "config.angular2-files.menu.module",
                    "command": "extension.addAngular2Module"
                },
                {
                    "when": "config.angular2-files.menu.entity",
                    "command": "extension.addAngular2Entity"
                }
            ]
        }
    },
    "devDependencies": {
        "@types/chai": "^4.2.12",
        "@types/mocha": "^8.0.1",
        "@types/node": "^14.0.27",
        "@types/rimraf": "^3.0.0",
        "@types/sinon": "^9.0.4",
        "@types/sinon-chai": "^3.2.4",
        "chai": "^4.2.0",
        "copyfiles": "^2.3.0",
        "mocha": "^8.1.1",
        "rimraf": "^3.0.2",
        "sinon": "^9.0.2",
        "sinon-chai": "^3.5.0",
        "tslint": "^6.1.3",
        "tslint-config-airbnb": "^5.11.2",
        "typescript": "^3.9.7",
        "vscode": "^1.1.37",
        "vscode-test-content": "^1.2.0"
    },
    "dependencies": {
        "dirty-json": "^0.9.1",
        "express-es6-template-engine": "^2.2.3"
    }
}
